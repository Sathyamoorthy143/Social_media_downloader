# Backend Environment Variables
# Copy this file to .env and update the values as needed

# Debug mode (set to False in production)
DEBUG=True

# Directory for temporary file storage
TEMP_DIR=temp_files

# File expiration time in seconds (default: 30 minutes)
EXPIRATION=1800

# Maximum download size in bytes (default: 2GB)
MAX_SIZE=2147483648

# Server port (for production deployment)
# PORT=5000

# Optional: Proxies (comma-separated list)
# PROXIES=

# Optional: Authentication settings
# AUTH=False
# ACCESS_TOKEN=
# REFRESH_TOKEN=
# EXPIRES=
# VISITOR_DATA=
# PO_TOKEN=
